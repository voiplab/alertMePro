<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:shemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>AlertMeNotificationPlugin</name>
    <display-name>AlertMe Notification</display-name>
    <version>@Version@</version>
    <description>A TeamCity Plugin which allows to send notifications to Skype group chats and direct messages.</description>
    <download-url>http://alertme.pro/download</download-url>
    <email>support@voip-lab.ru</email>
    <vendor>
      <name>@VendorName@</name>
      <url>@VendorURL@</url>
      <logo></logo>
    </vendor>
  </info>
  <deployment use-separate-classloader="true" />
</teamcity-plugin>
